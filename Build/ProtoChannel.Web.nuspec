<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ProtoChannel.Web</id>
    <version>$version$</version>
    <authors>GMT Europe B.V.</authors>
    <description>.NET TCP/IP protocol based on Protobuf.Net with support for asynchronous AJAX communication.</description>
    <projectUrl>https://github.com/gmt-europe/ProtoChannel</projectUrl>
    <licenseUrl>http://www.gnu.org/licenses/lgpl-3.0.html</licenseUrl>
    <dependencies>
      <dependency id="Common.Logging" />
      <dependency id="protobuf-net" />
      <dependency id="ProtoChannel" />
      <dependency id="Newtonsoft.Json" />
    </dependencies>
  </metadata>
  <files>
    <file src="..\ProtoChannel.Web\bin\Release\ProtoChannel.Web.dll" target="Lib\Net40" />
    <file src="..\ProtoChannel.CodeGenerator\bin\Release\*.exe" target="Tools" />
    <file src="..\ProtoChannel.HttpProxy\prototype.js" target="Content" />
    <file src="..\ProtoChannel.HttpProxy\protochannel.js" target="Content" />
    <file src="..\ProtoChannel.HttpProxy\Web.config" target="Content" />
  </files>
</package>
